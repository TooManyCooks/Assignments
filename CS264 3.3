#include <iostream>

using namespace std;

int main() {
    
    int x;
 
    cout << "Enter a five digit number: ";
    cin >>x;
 
    if (x < 10000 || x > 99999) {
        cout << "Not five digits long, scrub..." << endl;
        return 1;
    }
 
    cout << x / 10000 << "   ";
    cout << (x / 1000) % 10 << "   ";
    cout << (x / 100) % 10 << "   ";
    cout << (x / 10) % 10 << "   ";
    cout << x % 10 << endl;
 
    return 0;
}
