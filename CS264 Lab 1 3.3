#include <iostream>

using namespace std;

int main() {
    
    int x;
 
    cout << "Enter a five digit number: ";
    cin >>x;
 
    if (x < 10000 || x > 99999) {
        cout << "Not five digits long, scrub..." << endl;
        return 1;
    }
 
    for(int i = 10000; i > 0; i = i/10){
        cout << (x / i) % 10 << "   ";
    }
    
    return 0;
}
