#include <iostream>

bool isMultiple(int x, int y){
    if(x % y == 0){
        return true;
    }
    else{
        return false;
    }
}

int main()
{
    using namespace std;
    
    int x, y;
    cin >> x >> y;
    if(isMultiple(x,y) == 1){
        cout << x << " is a multiple of " << y;   
    }
    else{
     cout << x << " is NOT a multiple of " << y;   
    }
    
    return 0;
}
