#include <iostream>

using namespace std;

int gcd(int x, int y){
    for(int i = x; i > 0; i--){
        if(x % i == 0 && y % i == 0){
            return i;
        }
    }
    return -1;
}

int main()
{
    int x, y;
    cout << "Enter two numbers : ";
    cin >> x >> y;
    
    int result = gcd(x, y);
    cout << "Greatest common divisor is " << result << endl;
    
    return 0;
}
